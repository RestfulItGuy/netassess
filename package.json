    "firebase": "^7.6.1",
    "react": "^16.6.3",
    "react-devtools-electron": "^4.7.0",
    "react-dom": "^16.6.3",
    "react-loader-spinner": "^3.1.14",
    "react-router-dom": "4.3.1",
